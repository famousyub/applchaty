{
  "Serilog": {  
    "MinimumLevel": {  
        "Default": "Information",  
        "Override": {  
            "Microsoft": "Information",  
            "System": "Warning",  
            "System.Net.Http.HttpClient": "Warning",
            "Microsoft.AspNetCore.Server.Kestrel" : "Information"
        }
    },
    "Using": ["Serilog.Sinks.Console"],
    "WriteTo": [
      {
        "Name" : "Console"
      }
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
